{
	"name": "Angular Template Helper",
	"displayName": "test-app",
	"description": "openfiles",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:angularTemplateHelper.openAsSplit"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "angularTemplateHelper.openAsSplit",
				"title": "Angular Template Helper"
			}
		],
		"keybindings": {
			"key": "Shift+Alt+O",
			"command": "angularTemplateHelper.openAsSplit"
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"vsce": "^2.6.3"
	}
}
